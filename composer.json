{
  "name": "US/Soporteav",
  "description": "Skeleton for develop web applications based on silex and doctrine at University of Seville",
  "license": "MIT",
  "type": "project",
  "require-dev": {
    "phpunit/php-invoker": "~1.1",
    "phpunit/phpunit": "3.7.*",
    "symfony/browser-kit": "^3.0",
    "symfony/css-selector": "^3.0"
  },
  "require": {
    "dflydev/doctrine-orm-service-provider": "2.0.*@dev",
    "monolog/monolog": "~1.17",
    "php": ">=5.5.9",
    "silex/silex": "~2.0",
    "silex/web-profiler": "~2.0",
    "symfony/class-loader": "~3.0",
    "symfony/config": "~3.0",
    "symfony/console": "~3.0",
    "symfony/debug": "~3.0",
    "symfony/doctrine-bridge": "~3.0",
    "symfony/finder": "~3.0",
    "symfony/form": "~3.0",
    "symfony/monolog-bridge": "~3.0",
    "symfony/process": "~3.0",
    "symfony/security": "~3.0",
    "symfony/twig-bridge": "~3.0",
    "symfony/validator": "~3.0",
    "symfony/yaml": "~3.0"
  },
  "autoload": {
    "psr-4": {
      "US\\Soporteav\\": "src/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "scripts": {
    "run": [
      "echo 'Started web server on http://localhost:8888'",
      "php -S localhost:8888 -t web web/index_dev.php"
    ]
  }
}
